{
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "eslint.autoFixOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.removeUnusedImports": "explicit"
    },
    "git.autofetch": true,
    "files.autoSaveDelay": 500,
    "tabnine.experimentalAutoImports": true,
    "glassit.alpha": 220,
    "extensions.ignoreRecommendations": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
        
    },
    "files.readonlyInclude": {
      "node_modules/**": true
    },
    "workbench.preferredLightColorTheme": "Default Dark+",
    "workbench.iconTheme": "vscode-icons",
    "window.systemColorTheme": "auto",
    "workbench.preferredDarkColorTheme": "1337",
    "window.autoDetectColorScheme": true,
    "editor.fontSize": 13,
    "debug.console.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "markdown.preview.fontSize": 13,
    "scm.inputFontSize": 13,
    "chat.editor.fontSize": 13,
    "editor.suggestFontSize": 13,
    "gitlens.blame.fontSize": 13,
    "editor.codeLensFontSize": 13,
    "fontis": 14,
    "editor.tabSize": 2,
    "editor.lineHeight": 1.712,
    "workbench.tree.indent": 15,
    "workbench.startupEditor": "none",
    "window.openFoldersInNewWindow": "on",
    "editor.fontLigatures": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "cSpell.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.compactFolders": false,
    "gitlab.showPipelineUpdateNotifications": true,
    "editor.accessibilitySupport": "off",
    "workbench.settings.applyToAllProfiles": [
        "javascript.experimental.updateImportsOnPaste"
    ],
    "security.promptForLocalFileProtocolHandling": false,
    "explorer.confirmPasteNative": false,
    "gitlens.ai.experimental.model": "openai:gpt-4o",
    "eslint.codeActionsOnSave.rules": null,
    "search.exclude": {
        ".next": true,
        ".vite": true,
        "**/.next": true,
        "**/.vite": true
    },
    "files.exclude": {
        ".next": true,
        ".vite": true,
        "**/.next": true,
        "**/.vite": true
    },
    "explorer.confirmDragAndDrop": false,
    "workbench.colorTheme": "Default Light+",
    "settingsSync.ignoredSettings": [],
    "window.zoomLevel": 1.3,
    "git.openRepositoryInParentFolders": "always",
    "errorLens.decorations": {
        "errorRange": {},
        "warningRange": {},
        "infoRange": {},
        "hintRange": {}
    },
    "errorLens.gutterIconsEnabled": true,
    "errorLens.gutterIconSet": "emoji",
    "errorLens.gutterEmoji": {
        "error": "üêΩ"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "gitlens.views.tags.files.layout": "tree",
    "css.customData": ["tailwind.json"],
    "scss.lint.unknownAtRules": "ignore",
    "css.lint.unknownAtRules": "ignore",
    "workbench.fontAliasing": "auto",
    "typelens.showReferencesForClasses": true,
    "typelens.showReferencesForFunctions": true,
    "typelens.showReferencesForInterfaces": true,
    "typelens.showReferencesForMethods": false,
    "typelens.showReferencesForProperties": false,
    "tailwindCSS.classAttributes": ["class", "className", "ngClass", "class:list", "classNames"],
    "tailwindCSS.experimental.classRegex": [
        ["classNames\\(([^)]+)\\)", "'([^']*)'"],
        ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
        ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
    ],
    "files.autoSave": "afterDelay",
    "workbench.editor.enablePreview": false,
    "window.customTitleBarVisibility": "auto",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "codeium.enableCodeLens": false,
    "eslint.format.enable": true,
    "eslint.run": "onSave"
}